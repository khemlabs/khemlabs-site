<div id="contact" class="section white z-depth-1 flow-text" style="margin-top: 25px;">
  <div class="container">
    <div class="row">
      <h2 class="header red-text tex-darken-4"><%= __({phrase: 'menu_contact', locale: lang}) %></h2>
    </div>
    <div class="row">
      <form class="col s12" id="message-form">
        <div class="row">
          <div class="input-field col s6">
            <input name="first_name" id="first_name" type="text" class="validate" required>
            <label for="first_name"><%= __({phrase: 'name_contact', locale: lang}) %></label>
          </div>
          <div class="input-field col s6">
            <input name="company" id="company" type="text" class="validate" required>
            <label for="company_name"><%= __({phrase: 'company_contact', locale: lang}) %></label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input name="email" id="email" type="email" class="validate" required>
            <label for="email"><%= __({phrase: 'email_contact', locale: lang}) %></label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <textarea name="textbody" id="textbody" class="materialize-textarea" required></textarea>
            <label for="textbody"><%= __({phrase: 'message_contact', locale: lang}) %></label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12 red-text tex-darken-4">
            <button id="send-btn" class="waves-effect red btn"><i class="material-icons left">comment</i>
              <%= __({phrase: 'send_contact', locale: lang}) %>
            </button>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>